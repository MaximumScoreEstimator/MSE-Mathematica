             RowBox[{"[", "m", "]"}], "]"}], "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}], "]"}], ",", "d"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"payoffMatrix", "[", 
        RowBox[{"[", "m", "]"}], "]"}], "=", 
       RowBox[{"payoffMatrix", "[", 
